extends layout

block content

  .utilisation-container
    .utilisation.js-utilisation(data-first-month=months[0], data-last-month=months[1])
    .utilisation-change.js-utilisation-change
      h1.js-utilisation-title
      h2 Utilisation
      p.dates-listed
        span.js-two-month-title
        |  - 
        span.js-last-month-title

      p.last-month
        span
          strong.js-this-month
          |  last month

  script(src='//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js')
  script(src='/javascripts/index.js')
